<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sb.warda.languageapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="28" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.sb.warda.languageapp.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.sb.warda.languageapp.Numbers"
            android:label="@string/numbers" />
        <activity
            android:name="com.example.sb.warda.languageapp.Family"
            android:label="@string/family" />
        <activity
            android:name="com.example.sb.warda.languageapp.Colors"
            android:label="@string/colors" />
        <activity
            android:name="com.example.sb.warda.languageapp.Phrases"
            android:label="@string/phrases" />
    </application>

</manifest>